	int first = 1;
	int second = 5;
	int third = 1;

	if (first == second || first == third || second == third) {
		cout << "there are equal numbers" << endl;
	}
	if (first > second && first > third) {
		cout << first << "-maksimalais" << endl;
	}
	else if (second > first && second > third) {
		cout << second << "-maksimalais" << endl;
	}
	else if (third > first && third > second) {
		cout << third << "-maksimalais" << endl;
	}
	if (first < second && first < third) {
		cout << first << "-minimalais" << endl;
	}
	else if (second < first && second < third) {
		cout << second << "-minimalais" << endl;
	}
	else if (third < first && third < second) {
		cout << third << "-minimalais" << endl;
	}

	return 0;
